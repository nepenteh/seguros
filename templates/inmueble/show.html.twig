{% extends 'base.html.twig' %}

{% block title %}Inmueble{% endblock %}

{% block body %}
<div class="container py-1 pt-3">
    <h1>Inmueble</h1>

    <table class="table table-striped mt-3">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ inmueble.id }}</td>
            </tr>
            <tr>
                <th>Calle</th>
                <td>{{ inmueble.calle }}</td>
            </tr>
            <tr>
                <th>Cp</th>
                <td>{{ inmueble.cp }}</td>
            </tr>
            <tr>
                <th>Localidad</th>
                <td>{{ inmueble.localidad }}</td>
            </tr>
            <tr>
                <th>Provincia</th>
                <td>{{ inmueble.provincia }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-outline-dark py-1" href="{{ path('app_inmueble_index') }}">Volver al Listado</a>

    <a class="btn btn-outline-dark py-1" href="{{ path('app_inmueble_edit', {'id': inmueble.id}) }}">Edit</a>

    {{ include('inmueble/_delete_form.html.twig') }}
</div>
{% endblock %}
