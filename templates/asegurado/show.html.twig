{% extends 'base.html.twig' %}

{% block title %}Asegurado{% endblock %}

{% block body %}
<div class="container py-1 pt-3">
    <h1>Asegurado</h1>

    <table class="table table-striped mt-3">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ asegurado.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ asegurado.nombre }}</td>
            </tr>
            <tr>
                <th>Apellido1</th>
                <td>{{ asegurado.apellido1 }}</td>
            </tr>
            <tr>
                <th>Apellido2</th>
                <td>{{ asegurado.apellido2 }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-outline-dark py-1" href="{{ path('app_asegurado_index') }}">Volver al Listado</a>

    <a class="btn btn-outline-dark py-1" href="{{ path('app_asegurado_edit', {'id': asegurado.id}) }}">Edit</a>

    {{ include('asegurado/_delete_form.html.twig') }}
</div>
{% endblock %}
